   <bottom>
      <HBox alignment="CENTER" prefHeight="10.0" prefWidth="200.0" spacing="10.0" style="-fx-background-color: FBD9E3;" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="searchid" mnemonicParsing="false" prefWidth="80.0" style="-fx-background-color: E17295;" text="조회" textFill="WHITE" />
            <Button id="findidCloseBtn" mnemonicParsing="false" prefWidth="80.0" style="-fx-background-color: E17295;" text="닫기" textFill="WHITE" />
         </children>
         <BorderPane.margin>
