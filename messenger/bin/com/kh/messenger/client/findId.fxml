<BorderPane prefHeight="130.0" prefWidth="230.0" stylesheets="@memjoin.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kh.messenger.client.SearchIdPwController">
   <center>
